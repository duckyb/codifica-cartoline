      <zone xml:id="" start="" ulx="" uly="" lrx="" lry="" rotate=""/>
      <!-- seconda pagina - testo -->
      <zone xml:id="" start="" points="" rotate=""/>
      <zone xml:id="" start="" points="" rotate=""/>
      <zone xml:id="" start="" points="" rotate=""/>
      <zone xml:id="" start="" points="" rotate=""/>
      <zone xml:id="" start="" points="" rotate=""/>
      <zone xml:id="" start="" points="" rotate=""/>
      <zone xml:id="" start="" points="" rotate=""/>
      <!-- 11 -->
      <zone xml:id="" start="" ulx="" uly="" lrx="" lry="" rotate=""/>
      <!-- 1006 -->
      <zone xml:id="" start="" ulx="" uly="" lrx="" lry="" rotate=""/>
      <!-- timbro SX -->
      <zone xml:id="" start="" points="" rotate=""/>
      <!-- timbro DX e francobollo-->
      <zone xml:id="" start="" points="" rotate=""/>
    </surface>
    <!-- immagine | fronte della cartolina -->
    <surface>
      <graphic xml:id="" url="" width="" height=""/>
      <!-- firma dell'autore dell'immagine -->
      <zone xml:id="" start="" ulx="" uly="" lrx="" lry="" rotate=""/>
    </surface>
  </facsimile>
  <text xml:lang="">
    <!-- timbri e francobolli -->
    <front/>
    <body>
      <!-- ............ FRONTE | IMG | RECTO ............ -->
      <div type="" part="" org="" sample="">
        <figure>
          <!-- Descrizione dell'immagine in linguaggio naturale -->
          <figDesc/>
        </figure>
      </div>
      <!-- ............ RETRO | TEXT | VERSO ............ -->
      <div type="" xml:id="" facs="" part="" org="" sample="">
        <div type="" part="" org="" sample="">
          <!--luogo e data di scrittura della cartolina-->
          <opener xml:id="" facs="">
            <dateline rend="">
              <placeName ref="" instant="" full=""/>
              <date when="" instant=""/>
            </dateline>
            <salute>
              <s part="">
                <lb xml:id="" facs=""/>
                <lb xml:id="" facs=""/>
              </s>
              <s part="">
                <lb xml:id="" facs=""/>
                <lb xml:id="" facs=""/>
              </s>
            </salute>
          </opener>
          <!--saluto iniziale-->
          <!--contenuto del messaggio-->
          <p part="">
            <s xml:id="" facs="" part=""/>
            <s xml:id="" facs="" part="">
              <country/>
            </s>
          </p>
          <!--saluto finale e firma-->
          <p xml:id="" facs="" part="">
            <!-- Testo ruotato 90Â° -->
            <s type="" style="" part="">
              <unclear reason="" agent="" cert="" instant=""/>
            </s>
            <s type="" style="" part="">
              <orgName key="" type="" instant="" full="">
                <persName instant="" full="">
                  <surname full=""/>
                </persName>
                <placeName key="" instant="" full=""/>
              </orgName>
            </s>
          </p>
          <closer rend="">
            <!-- Firma del mittente -->
            <lb xml:id="" facs=""/>
            <signed xml:id="" facs="">
              <persName ref="" instant="" full="">
                <forename full=""/>
                <surname full=""/>
              </persName>
            </signed>
          </closer>
        </div>
        <div type="" part="" org="" sample="">
          <div n="" part="" org="" sample="">
            <ab part="">
              <!-- Timbro SX -->
              <stamp type="" style="">
                <mentioned rend="">
                  <gap reason="" agent="" atLeast="" atMost="" unit="" instant=""/>
                  <mentioned>
                    <hi style="">
                      <unclear reason="" agent="" cert="" instant=""/>
                      <gap reason="" agent="" atLeast="" atMost="" unit="" instant=""/>
                    </hi>
                  </mentioned>
                </mentioned>
              </stamp>
              <!-- Timbro DX -->
              <stamp type="">
                <mentioned rend="">
                  <gap reason="" agent="" atLeast="" atMost="" unit="" instant=""/>
                  <mentioned>
                    <hi style="">
                      <unclear reason="" agent="" cert="" instant=""/>
                      <unclear reason="" agent="" cert="" instant=""/>
                      <gap reason="" agent="" atLeast="" atMost="" unit="" instant=""/>
                    </hi>
                  </mentioned>
                </mentioned>
              </stamp>
              <!-- Francobollo -->
              <stamp type="" style="">
                <mentioned style="">
                  <hi/>
                  <hi>
                    <hi rend=""/>
                    <hi rend=""/>
                  </hi>
                </mentioned>
              </stamp>
            </ab>
          </div>
          <div n="" part="" org="" sample="">
            <p part="">
              <!--address type="receiver"-->
              <address>
                <!-- Righe della sezione: Destinatario -->
                <addrLine xml:id="" facs=""/>
                <addrLine xml:id="" facs="">
                  <persName instant="" full="">
                    <surname full=""/>
                    <forename full=""/>
                  </persName>
                </addrLine>
                <addrLine xml:id="" facs=""/>
                <addrLine xml:id="" facs=""/>
                <addrLine xml:id="" facs=""/>
                <addrLine xml:id="" facs=""/>
              </address>
            </p>
          </div>
        </div>
      </div>
    </body>
  </text>
</TEI>
