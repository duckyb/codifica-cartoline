        </respStmt>
      </editionStmt>
      <publicationStmt>
        <publisher/>
        <pubPlace/>
        <availability default="">
          <licence target=""/>
        </availability>
      </publicationStmt>
      <sourceDesc default="">
        <bibl type="" default="" status="">
          <author>
            <!-- autore (mittente) della cartolina -->
            <persName ref="" instant="" full="">
              <forename full=""/>
              <surname full=""/>
            </persName>
          </author>
          <!-- editore della cartolina -->
          <date xml:id="" when="" instant=""/>
          <publisher/>
          <pubPlace/>
        </bibl>
        <msDesc status="">
          <msIdentifier>
            <country/>
            <settlement xml:id=""/>
            <idno type=""/>
          </msIdentifier>
          <msContents>
            <msItem>
              <textLang xml:id="" mainLang=""/>
            </msItem>
          </msContents>
          <physDesc>
            <objectDesc>
              <supportDesc>
                <support>
                  <material/>
                  <!-- descrizione fisica dei timbri -->
                  <stamp type=""/>
                  <!-- descrizione fisica del francobollo -->
                  <stamp type=""/>
                  <!-- descrizione fisica della cartolina -->
                  <p part=""/>
                </support>
                <!-- grandezza appossimativa del documento elettronico -->
                <extent>
                  <measure type="" unit="" quantity=""/>
                  <dimensions unit="">
                    <height/>
                    <width/>
                  </dimensions>
                </extent>
                <!-- descrizione delle condizioni fisiche di conservazione della cartolina -->
                <condition>
                  <p part=""/>
                </condition>
              </supportDesc>
            </objectDesc>
            <!-- descrizione del tipo di scrittura usato nella cartolina -->
            <handDesc>
              <p part=""/>
            </handDesc>
          </physDesc>
          <!-- ??Note: Completare o rimuovere <additional /> -->
          <!-- <additional>
						<adminInfo>
							<note></note>
						</adminInfo>
					</additional> -->
