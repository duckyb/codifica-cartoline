      <!--graphic xml:id="retro_postcard" start="#retro-postcard" url="'retro.jpg'" width="'1628px'" height="'1053px'" /-->
      <graphic xml:id="" url="" width="" height=""/>
      <!-- prima pagina, lato sx -->
      <zone xml:id="" start="" points="" rotate=""/>
      <zone xml:id="" start="" points="" rotate=""/>
      <zone xml:id="" start="" points="" rotate=""/>
      <zone xml:id="" start="" points="" rotate=""/>
      <zone xml:id="" start="" points="" rotate=""/>
      <zone xml:id="" start="" points="" rotate=""/>
      <zone xml:id="" start="" points="" rotate=""/>
      <zone xml:id="" start="" points="" rotate=""/>
      <zone xml:id="" start="" points="" rotate=""/>
